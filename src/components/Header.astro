<header>
    <div class="h-14 border-b border-color-subtle flex items-center px-6">
        <a
            aria-current="page"
            href="/"
            class="router-link-active router-link-exact-active flex items-center space-x-1 mr-3 sm:mr-6"
            ><div
                class="logo-favicon rounded-lg text-lg flex items-center justify-center font-semibold w-8 h-8"
            >
                Anti
            </div><div
                class="text-color font-medium text-xl tracking-normal hidden md:block"
            >
                NP
            </div></a
        ><div class="flex items-center space-x-3">
            <a
                aria-current="page"
                href="/"
                class="router-link-active router-link-exact-active router-link flex"
            >
                Inicio
            </a><a href="/app" class="router-link flex"> Agenda</a>
            <a href="/about" class="router-link flex"> Acerca de!</a><a
                class="router-link hidden sm:flex"
                href="https://www.facebook.com/brybor.sh/"
                target="_blank"
            >
                Nosotros <svg
                    xmlns="http://www.w3.org/2000/svg"
                    fill="none"
                    viewBox="0 0 24 24"
                    stroke-width="1.5"
                    stroke="currentColor"
                    class="w-5 h-5 ml-2"
                    ><path
                        stroke-linecap="round"
                        stroke-linejoin="round"
                        d="M13.5 6H5.25A2.25 2.25 0 003 8.25v10.5A2.25 2.25 0 005.25 21h10.5A2.25 2.25 0 0018 18.75V10.5m-10.5 6L21 3m0 0h-5.25M21 3v5.25"
                    ></path></svg
                ></a
            >
        </div><div class="ml-auto flex items-center space-x-4">
            <!--Dark Mode Config HTML -->
            <div class="cursor-pointer">
                <button id="theme-toggle" type="button" class="text-gray-500 dark:text-gray-400 hover:bg-gray-100 dark:hover:bg-gray-700 focus:outline-none focus:ring-4 focus:ring-gray-200 dark:focus:ring-gray-700 rounded-lg text-sm p-2.5">
                    <svg id="theme-toggle-dark-icon" class="hidden w-5 h-5" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M17.293 13.293A8 8 0 016.707 2.707a8.001 8.001 0 1010.586 10.586z"></path></svg>
                    <svg id="theme-toggle-light-icon" class="hidden w-5 h-5" fill="currentColor" viewBox="0 0 20 20" xmlns="http://www.w3.org/2000/svg"><path d="M10 2a1 1 0 011 1v1a1 1 0 11-2 0V3a1 1 0 011-1zm4 8a4 4 0 11-8 0 4 4 0 018 0zm-.464 4.95l.707.707a1 1 0 001.414-1.414l-.707-.707a1 1 0 00-1.414 1.414zm2.12-10.607a1 1 0 010 1.414l-.706.707a1 1 0 11-1.414-1.414l.707-.707a1 1 0 011.414 0zM17 11a1 1 0 100-2h-1a1 1 0 100 2h1zm-7 4a1 1 0 011 1v1a1 1 0 11-2 0v-1a1 1 0 011-1zM5.05 6.464A1 1 0 106.465 5.05l-.708-.707a1 1 0 00-1.414 1.414l.707.707zm1.414 8.486l-.707.707a1 1 0 01-1.414-1.414l.707-.707a1 1 0 011.414 1.414zM4 11a1 1 0 100-2H3a1 1 0 000 2h1z" fill-rule="evenodd" clip-rule="evenodd"></path></svg>
                </button>
            </div>
            <div class="flex items-center space-x-3">
                <button
                    class="text-sm px-4 py-2 rounded-full bg-primary-button text-white font-medium"
                >
                    <span class="hidden sm:block">Iniciar sesion</span>
                    <svg
                        class="w-5 h-5 sm:hidden"
                        xmlns="http://www.w3.org/2000/svg"
                        fill="none"
                        viewBox="0 0 24 24"
                        stroke-width="1.5"
                        stroke="currentColor"
                        ><path
                            stroke-linecap="round"
                            stroke-linejoin="round"
                            d="M15.75 6a3.75 3.75 0 11-7.5 0 3.75 3.75 0 017.5 0zM4.501 20.118a7.5 7.5 0 0114.998 0A17.933 17.933 0 0112 21.75c-2.676 0-5.216-.584-7.499-1.632z"
                        ></path>
                    </svg>
                </button>
            </div>
        </div>
    </div>
</header>
